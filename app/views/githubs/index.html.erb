<h1>Listing githubs</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @githubs.each do |github| %>
  <tr>
    <td><%= link_to 'Show', github %></td>
    <td><%= link_to 'Edit', edit_github_path(github) %></td>
    <td><%= link_to 'Destroy', github, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

